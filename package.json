{
  "name": "hnews-ui",
  "version": "1.0.0",
  "private": true,
  "description": "HackerNews client with GraphQL, Semantic-UI, Typescript and Mobx.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --config=./webpack.config.js",
    "start2": "webpack-dev-server -d --history-api-fallback --colors --progress --port 3000",
    "build": "webpack -p --progress --colors",
    "serve": "pushstate-server build 9000",
    "gql-gen": "gql-gen --url http://localhost:8080/graphql --template typescript-single --out ./src/api/typings.ts ./src/**/*.graphql",
